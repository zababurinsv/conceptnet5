{
  "name": "@orbit/concept",
  "version": "1.0.0",
  "description": "index",
  "main": "src/index.mjs",
  "private": true,
  "type": "module",
  "author": "Zababurin Sergey",
  "license": "Apache-2.0",
  "directories": {
    "doc": "/"
  },
  "config": {
    "env": "zb"
  },
  "scripts": {
    "dev": "parcel web/conceptnet_web/templates/index.html",
    "dev:star_initiator": "yarn workspace @packages/star dev:initiator",
    "dev:star_dialer": "yarn workspace @packages/star dev:dialer",
    "server": "yarn --cwd backend start",
    "build": "yarn --cwd frontend/react build",
    "test:react": "yarn --cwd frontend/react build",
    "download": "./scripts/service-initialization.sh",
    "dev:up:task": "yarn workspace @orbit/task dev",
    "dev:up:init": "yarn workspace @orbit/init dev",
    "dev:up:test": "yarn workspace @orbit/test dev",
    "build:up:task": "yarn workspace @orbit/task build",
    "build:up:init": "yarn workspace @orbit/init build",
    "build:up:test": "yarn workspace @orbit/test build",
    "git:push": "mr --trust-all push",
    "git:pull": "mr --trust-all pull",
    "config:mr:trust": "sudo nano ~/.mrtrust",
    "config:mr": "sudo nano ~/.mrconfig",
    "clean:All": "find . -name \"node_modules\" -exec rm -rf '{}' +; find . -name \"package-lock.json\" -exec rm -rf '{}' +;  find . -name \"yarn.lock\" -exec rm -rf '{}' +;",
    "clean:Parcel": "find . -name \".parcel-cache\" -exec rm -rf '{}' +; ",
    "clean:idea": "find . -name \".idea\" -exec rm -rf '{}' +; "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zababurinsv/zababurinsv.github.ob.git"
  },
  "keywords": [
    "repository"
  ],
  "bugs": {
    "url": "https://github.com/zababurinsv/zababurinsv.github.ob/issues"
  },
  "homepage": "https://zababurinsv.github.ob/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "maintainers": [],
  "workspaces": [
    "src/**",
    "packages/*"
  ],
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@newkind/ts-types": "^0.1.0",
    "@orbit/config": "^1.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/ramda": "^0.27.45",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.0",
    "babel-jest": "^26.6.0",
    "babel-loader": "8.1.0",
    "eslint": "^7.11.0",
    "parcel": "2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "webpack": "4.44.2"
  },
  "dependencies": {
    "@newkind/universe": "^0.1.0"
  }
}
