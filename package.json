  "maintainers": [],
  "workspaces": [
    "src/**",
    "packages/*"
  ],
  "devDependencies": {
    "@parcel/resolver-default": "^2.0.0-alpha.3",
    "parcel": "2.0.0-rc.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
