{
  "name": "@orbit/test",
  "version": "1.0.0",
  "description": "task manager for async events",
  "main": "./dist/index.mjs",
  "author": "Zababurin Sergey",
  "license": "GPL-3.0-only",
  "private": true,
  "config": {
    "port": 8426
  },
  "scripts": {
    "start": "start",
    "dev": "parcel ./src/index.html -p $npm_package_config_port",
    "build": "parcel build ./src/index.mjs",
    "kill": "kill $(lsof -t -i:$npm_package_config_port)",
    "clean": "rimraf ../../.parcel-cache"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@parcel/resolver-default": "^2.0.0-alpha.3",
    "@parcel/transformer-sass": "2.0.0-rc.0",
    "npm-run-all": "^4.1.5",
    "parcel": "2.0.0-rc.0",
    "parcel-plugin-static-files-copy": "^2.6.0"
  },
  "dependencies": {
    "chai": "^4.3.4"
  }
}
